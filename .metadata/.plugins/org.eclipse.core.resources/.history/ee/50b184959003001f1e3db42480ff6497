package chap03_operator;

public class _03_LogicalOperator {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		// 1. 관계연산자(비교연산자)
		int num1 = 110;
		int num2 = 225;
		
		// true
		System.out.println("num1 < num2 : " + (num1 < num2));
		// true
		System.out.println("num <= num2 : " + (num1 <= num2));
		// false
		System.out.println("num1 > num2 : " + (num1 > num2));
		// false
		System.out.println("num1 >= num2 : " + (num1 >= num2));
		// false
		System.out.println("num1 == num2 : " + (num1 == num2));
		// true
		System.out.println("num1 != num2 : " + (num1 != num2));
	}

}

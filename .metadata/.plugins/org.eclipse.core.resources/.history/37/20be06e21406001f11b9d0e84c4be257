package homework;

public class _05_Advance1_3 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		System.out.println("6-1)");
		for(int i = 0; i < 5; i++) {
			for(int j = 0; j < 5; j++) {
				if(j > i) {
					System.out.println();
					break;
				}
				System.out.print("*");
			}
		}
	}

}

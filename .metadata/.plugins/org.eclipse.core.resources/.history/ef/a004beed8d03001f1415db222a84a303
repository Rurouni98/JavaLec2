package test;

class Operator {
	int add(int a, int b) {
		int result;
		result = this.add(a, b);
		return result; 
	}
}

public class Test01 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Operator a = new Operator();
		int test = a.add(1,3);
		
		System.out.println(test);
	}

}

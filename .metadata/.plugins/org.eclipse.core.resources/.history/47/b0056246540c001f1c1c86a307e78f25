package homework.homework6.middle;

public class CharToWords {
	public static void main(String[] args) {
		char[] mess = {'b', 'c', 's', 'i', 'i', 'o', 'q', 'n', 't', 'm', 'l', 't'};
		
		String str1 = "";
		String str2 = "";
		String str3 = "";
		String str4 = "";
		
		for(int i = 0; i < mess.length; i++) {
			if(i % 4 == 0)
				str1 += mess[i];
		}
	}
}

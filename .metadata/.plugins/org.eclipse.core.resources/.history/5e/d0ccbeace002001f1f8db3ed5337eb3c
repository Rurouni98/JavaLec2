package chap02_variables;

public class _01_Variables {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		// 1. 변수의 선언
		// 1-1. 변수의 선언은 자료형 + 변수명
		// 좋아요 개수를 저장하는 변수
		int likeCnt;
		// 로그인한 사용자가 좋아요를 눌렀는지 여부를 저장하는 변수
		boolean isUserLike;
		
		
		// 1-2. 변수명 첫 글자는 문자, $, _만 허용된다.
		int price;
		int _bit;
		int $company;
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
	}

}

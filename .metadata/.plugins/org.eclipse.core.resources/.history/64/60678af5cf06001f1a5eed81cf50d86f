package homework;

public class _02_Basic {
	public static void main(String[] args) {
		
	}
	
	// 1번 문제
	public static int sum(int a) {
		int sum = 0;
		
		for(int i = 1; i <= a; i++) {
			sum += i;
		}
		
		return sum;
	}
	
	public String ConcatString(String a, String b) {
		return a + b;
	}
	
	
}

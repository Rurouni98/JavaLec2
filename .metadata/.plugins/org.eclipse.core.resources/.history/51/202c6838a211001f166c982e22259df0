package chap21_multithread;

public class _05_YieldMethod {
	static int flag = 0;
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Thread t1 = new Thread() {
			@Override
			public void run() {
				for(int i = 6; i <= 10; i++) {
					System.out.println(i);
				}
				
				flag = 1;
			}
		};
		
		t1.start();
	}

}

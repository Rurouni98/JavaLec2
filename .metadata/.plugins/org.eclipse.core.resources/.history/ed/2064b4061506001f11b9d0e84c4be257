package homework;

public class _05_Advance1_3 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		for(int i = 0; i < 5; i++) {
			for(int j = 0; j < 9; j++) {
				if(j >= 5 - i || j <= 5 + i) {
					System.out.print(" ");
					continue;
				}
				System.out.print("*");
			}
			System.out.println();
		}
	}

}

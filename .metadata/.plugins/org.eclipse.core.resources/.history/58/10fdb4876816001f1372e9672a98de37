			);
			
			char[] chArr = {'a', 'b', 'c', 'd', 'e'};
			
			for(int i = 0; i < chArr.length; i++) {
				// PrintStream은 출력 스트림에만 붙일 수 있는 
			}
			
			
			
			
			
			
			
			
			
			
			
			
		} catch(IOException ie) {
			System.out.println(ie.getMessage());
		}

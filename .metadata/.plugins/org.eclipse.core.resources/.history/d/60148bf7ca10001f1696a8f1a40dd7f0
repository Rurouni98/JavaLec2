package chap19_generic.ramyun;

public class Gimchi {
	private String color;
	
	public Gimchi() {
		this.color = "red";
	}

	public String getColor() {
		return color;
	}

	public void setColor(String color) {
		this.color = color;
	}

	@Override
	public String toString() {
		return "Gimchi [color=" + color + "]";
	}
}

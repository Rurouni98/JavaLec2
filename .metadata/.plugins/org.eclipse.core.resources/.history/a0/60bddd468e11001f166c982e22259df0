package homework.homework4.basic;

public class Developer extends Labor {

	@Override
	public void goWork() {
		// TODO Auto-generated method stub
		System.out.println("개발자님 출근하셔야 합니다.");
	}

	@Override
	public void leaveWork() {
		// TODO Auto-generated method stub
		System.out.println("개발자님 퇴근하셔도 됩니다.");
	}

	@Override
	public void work() {
		// TODO Auto-generated method stub
		System.out.println("개발자님 아직 더 일하셔야 합니다.");
	}

}
